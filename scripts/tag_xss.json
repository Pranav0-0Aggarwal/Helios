{
  "name": "Cross-Site Scripting (XSS)",
  "run_at": "request",
  "find": "many",
  "request": ["parameters", "cookies"],
  "data": {
    "inject_value": "<xss></xss>"
  },
  "matches": [
    {
      "type": "regex",
      "location": "body",
      "match": "<xss></xss>",
      "name": "Reflected HTML in body",
      "options": [
        "ignore_case"
      ]
    }
  ]
}